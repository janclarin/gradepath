    <style name="AppTheme" parent="android:Theme.Holo.Light">

        <!-- Customize your theme here. -->
        <item name="android:actionBarStyle">@style/CustomActionBar</item>
        <item name="android:windowContentOverlay">@null</item>
    </style>

    <style name="CustomActionBar" parent="android:Widget.Holo.Light.ActionBar.Solid.Inverse">
        <item name="android:windowActionBarOverlay">true</item>
        <item name="android:background">@android:color/holo_blue_light</item>
    </style>

    <style name="cardListView">
        <item name="android:paddingLeft">8dp</item>
        <item name="android:paddingRight">8dp</item>
        <item name="android:paddingTop">8dp</item>
        <item name="android:paddingBottom">72dp</item>
        <item name="android:divider">@null</item>
        <item name="android:dividerHeight">8dp</item>
        <item name="android:clipToPadding">false</item>
        <item name="android:scrollbarStyle">outsideOverlay</item>
        <item name="android:drawSelectorOnTop">true</item>
    </style>

    <style name="cardView">
        <item name="android:background">@drawable/bg_card</item>
        <item name="android:paddingBottom">2dp</item>
    </style>

    <style name="checkBoxDialog">
        <item name="android:layout_marginLeft">6dp</item>
        <item name="android:layout_marginTop">8dp</item>
        <item name="android:textAllCaps">true</item>
        <item name="android:textColor">@android:color/secondary_text_light_nodisable</item>
        <item name="android:textSize">14sp</item>
    </style>

    <style name="dialogPrompt">
        <item name="android:layout_gravity">center_horizontal</item>
        <item name="android:layout_marginBottom">48dp</item>
        <item name="android:paddingBottom">32dp</item>
        <item name="android:paddingLeft">8dp</item>
        <item name="android:paddingRight">8dp</item>
        <item name="android:paddingTop">8dp</item>
    </style>

    <style name="emptyListTextView" parent="android:Widget.Holo.Button.Borderless">
        <item name="android:padding">8dp</item>
        <item name="android:textColor">@android:color/tertiary_text_light</item>
        <item name="android:textSize">34sp</item>
        <item name="android:fontFamily">sans-serif-thin</item>
        <item name="android:gravity">center</item>
    </style>

    <style name="headerTextView">
        <item name="android:gravity">center_vertical|left</item>
        <item name="android:paddingLeft">12dp</item>
        <item name="android:textAllCaps">true</item>
        <item name="android:textColor">@android:color/secondary_text_light_nodisable</item>
    </style>

    <style name="spinnerButton" parent="android:Widget.Holo.Light.Spinner">
        <item name="android:layout_height">40dp</item>
        <item name="android:enabled">true</item>
        <item name="android:paddingLeft">12dp</item>
    </style>

